<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/register.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/admin/adminPage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/admin/addUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/admin/editUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>rentsForClient</from-outcome>
            <to-view-id>/admin/rentsForClient.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/admin/addUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/admin/confirmUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/admin/confirmUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/admin/addUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/admin/editUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/admin/rentsForClient.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/managerPageRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addBook</from-outcome>
            <to-view-id>/manager/addBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>editBook</from-outcome>
            <to-view-id>/manager/editBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addMovie</from-outcome>
            <to-view-id>/manager/addMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>editMovie</from-outcome>
            <to-view-id>/manager/editMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>rentsForCatalog</from-outcome>
            <to-view-id>/manager/rentsForCatalog.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/addBookRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addBook</from-outcome>
            <to-view-id>/manager/confirmBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/confirmBookRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/manager/addBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/addMovieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addMovie</from-outcome>
            <to-view-id>/manager/confirmMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/confirmMovieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/manager/addMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/editBookRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/editMovieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/manager/rentsForCatalog.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/client/clientPageRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addRent</from-outcome>
            <to-view-id>/client/addRent.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/client/addRent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>client</from-outcome>
            <to-view-id>/client/clientPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/client/clientPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>registered</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

</faces-config>
