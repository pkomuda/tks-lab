<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/src/main/webapp</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/src/main/webapp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/src/main/webapp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/html</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>rentsForClient</from-outcome>
            <to-view-id>/lient.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/html</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/er.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/er.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/html</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/lient.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/PageRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addBook</from-outcome>
            <to-view-id>/Rest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>editBook</from-outcome>
            <to-view-id>/kRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addMovie</from-outcome>
            <to-view-id>/eRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>editMovie</from-outcome>
            <to-view-id>/ieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>rentsForCatalog</from-outcome>
            <to-view-id>/rCatalog.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Rest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addBook</from-outcome>
            <to-view-id>/BookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/BookRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/Rest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/eRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addMovie</from-outcome>
            <to-view-id>/MovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/MovieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/eRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/kRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/ieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/rCatalog.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/PageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/geRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addRent</from-outcome>
            <to-view-id>/xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/xhtml</from-view-id>
        <navigation-case>
            <from-outcome>client</from-outcome>
            <to-view-id>/geRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/geRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp</from-view-id>
        <navigation-case>
            <from-outcome>registered</from-outcome>
            <to-view-id>/src/main/webapp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

</faces-config>
