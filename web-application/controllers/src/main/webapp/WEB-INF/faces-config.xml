<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/src/main/webapp/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/src/main/webapp/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/src/main/webapp/register.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/admin/adminPage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/src/main/webapp/admin/addUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/src/main/webapp/admin/editUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>rentsForClient</from-outcome>
            <to-view-id>/src/main/webapp/admin/rentsForClient.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/admin/addUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/src/main/webapp/admin/confirmUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/src/main/webapp/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/admin/confirmUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/src/main/webapp/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/src/main/webapp/admin/addUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/admin/editUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/src/main/webapp/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/src/main/webapp/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/admin/rentsForClient.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/src/main/webapp/admin/adminPage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/managerPageRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addBook</from-outcome>
            <to-view-id>/src/main/webapp/manager/addBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>editBook</from-outcome>
            <to-view-id>/src/main/webapp/manager/editBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addMovie</from-outcome>
            <to-view-id>/src/main/webapp/manager/addMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>editMovie</from-outcome>
            <to-view-id>/src/main/webapp/manager/editMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>rentsForCatalog</from-outcome>
            <to-view-id>/src/main/webapp/manager/rentsForCatalog.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/addBookRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addBook</from-outcome>
            <to-view-id>/src/main/webapp/manager/confirmBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/confirmBookRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/src/main/webapp/manager/addBookRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/addMovieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addMovie</from-outcome>
            <to-view-id>/src/main/webapp/manager/confirmMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/confirmMovieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancelConfirm</from-outcome>
            <to-view-id>/src/main/webapp/manager/addMovieRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/editBookRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/editMovieRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/manager/rentsForCatalog.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/src/main/webapp/manager/managerPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/client/clientPageRest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addRent</from-outcome>
            <to-view-id>/src/main/webapp/client/addRent.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/client/addRent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>client</from-outcome>
            <to-view-id>/src/main/webapp/client/clientPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/src/main/webapp/client/clientPageRest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/src/main/webapp/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>registered</from-outcome>
            <to-view-id>/src/main/webapp/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

</faces-config>
